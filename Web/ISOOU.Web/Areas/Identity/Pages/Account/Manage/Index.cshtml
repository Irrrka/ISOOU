@page
@model IndexModel
@{
    ViewData["Title"] = "Профил";
    ViewData["ActivePage"] = ManageNavPages.Index;
}

<partial name="_StatusMessage" for="StatusMessage" />

<div class="container">
    @{
        for (int i = 0; i < 1; i++)
        {
            <h3>Родители:</h3>
            <table class="table px-4 py-3">
                <tr height="18" style="height:18px;">
                    <td>Ирина Маринова</td>
                </tr>
                <tr height="18" style="height:18px;">
                    <td>Ивайло Маринов</td>
                </tr>
            </table>
        }
        <hr class="alert-heading" />
        for (int i = 0; i < 1; i++)
        {
            <h3>Деца:</h3>
            <table class="table px-4 py-3">
                <tr height="18" style="height:18px;">
                    <td>Явор Маринов</td>
                </tr>
                <tr height="18" style="height:18px;">
                    <td>Калин Маринов</td>
                </tr>
            </table>
        }
    }
</div>
